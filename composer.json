{
    "name": "bilaliqbalr/laravel-redis-auth",
    "description": "This package will provide redis based laravel auth driver",
    "keywords": [
        "bilaliqbalr",
        "laravel",
        "laravel-redis-auth"
    ],
    "homepage": "https://github.com/bilaliqbalr/laravel-redis-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Bilal Iqbal",
            "email": "bilaliqbalr@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/contracts": "^8.61",
        "illuminate/support": "^8.61",
        "predis/predis": "^1.1",
        "spatie/laravel-package-tools": "^1.4.3",
        "spatie/once": "^2.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Bilaliqbalr\\LaravelRedis\\": "src",
            "Bilaliqbalr\\LaravelRedis\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bilaliqbalr\\LaravelRedis\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bilaliqbalr\\LaravelRedis\\LaravelRedisServiceProvider"
            ],
            "aliases": {
                "LaravelRedis": "Bilaliqbalr\\LaravelRedis\\LaravelRedisFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
